{
  "compilerOptions": {
    /* Basic Options */
    // "incremental": true,
    "target": "es2017",                          
    "module": "commonjs",                     

    /* Strict Type-Checking Options */
    "strict": true,
    "esModuleInterop": true,

    /* Advanced Options */
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
